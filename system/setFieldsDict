FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 0
);

regions
(

    boxToCell
    {
        box (0 0 0) (0.6e-3 0.4e-3 1.8e-3);
        fieldValues
        (
    volScalarFieldValue alpha.metal 1
        );
    }

);


